      <script>
$(document).ready(function(){
     $( "input" ).keyup(function() {
        var eleme=this;
        var map = {
    '&': '',
    '<': '',
    '>': '',
    '"': '',
  };

    var newtxt=eleme.value.replace(/[&<>"']/g, function(m) { return map[m]; });
    document.getElementById(eleme.id).value=newtxt;
});
     });

   
   
    </script>
