<%- include('../../../templates/CVO-Homepage-NavBar.ejs') -%>
<script>
        document.getElementById('faqlink').className='btn bg-blue waves-effect';
        document.getElementById('faqlinkfont').color="white";
    </script>
<br><br><hr>
          <div class="row clearfix">
                                <div class="col-xs-12 ol-sm-12 col-md-12 col-lg-12">
                                    <b></b>
                                    <div class="panel-group" id="accordion_1" role="tablist" aria-multiselectable="false">
                                        <% faq.forEach(function (i) { %>
                                        <div class="panel panel-primary">
                                            <div class="panel-heading" role="tab" id="heading<%=i.int_FAQId%>">
                                                <h4 class="panel-title">
                                                    <a role="button" data-toggle="collapse" data-parent="#accordion_1" href="#<%= i.int_FAQId%>" aria-expanded="false" aria-controls="collapse<%=i.int_FAQId%>"  class="collapsed">
                                                        <%= i.str_Question%>
                                                    </a>
                                                </h4>
                                            </div>
                                            <% if (i.int_FAQId == 1) { %>
                                            <div id="<%= i.int_FAQId%>" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="heading<%=i.int_FAQId%>">
                                                <div class="panel-body">
                                                    <%= i.str_Answer %>
                                                </div>
                                            </div>
                                            <% } else { %>
                                                <div id="<%= i.int_FAQId%>" class="panel-collapse collapse" role="tabpanel" aria-labelledby="heading<%=i.int_FAQId%>">
                                                    <div class="panel-body">
                                                        <%= i.str_Answer %>
                                                    </div>
                                                </div>
                                            <% } %>
                                        </div>
                                     <% }); %>
                                    </div>
                                </div>
                                    </div>
<%- include('../../../templates/CVO-Homepage-Footer.ejs') -%>


