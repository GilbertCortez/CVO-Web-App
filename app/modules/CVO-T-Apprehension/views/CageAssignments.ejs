                                                        <tr>
                                                            <th>ID</th>
                                                            <th>Alias</th>
                                                            <th>Apprehended in</th>
                                                            <th>Apprehended Date</th>
                                                            <th>Species</th>
                                                            <th>Breed</th>
                                                            <th>Color</th>
                                                            <th>Action</th>
                                                        </tr>
                                                    </thead>
