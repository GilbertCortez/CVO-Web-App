                                  $('#modal_petPicture').attr('src', e.target.result);
                              };
                              reader.readAsDataURL(input.files[0]);
                            }
                            else{
                              $('#blah').attr('src',"../../images/animalPicture.jpg");
                            }
                        }
                     </script>
