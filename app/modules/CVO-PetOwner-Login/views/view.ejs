<body class="login-page" style="background-color:#2196f3">
    <div class="login-box">
        <div class="logo">
            <img src="../../images/image-gallery/CVO_LogoWhite.png" width="100%"/>
        </div>
        <div class="card">
            <div class="body">
            
                    <div class="msg">Pet Owner Login</div>
                    <form id="petowner_login" action="/PetOwner_Login" method="POST" onsubmit="return false;" >
                        <div class="input-group">
                            <span class="input-group-addon">
                                <i class="material-icons">person</i>
                            </span>
                            <div class="form-line">
                                <input type="text" class="form-control" name="email" placeholder="Email" value="gilbert@gmail.com" autofocus>
                            </div>
                        </div>
                        <div class="input-group">
                            <span class="input-group-addon">
                                <i class="material-icons">lock</i>
                            </span>
                            <div class="form-line">
                                <input type="password" class="form-control" name="password" placeholder="Password" value="sean">
                            </div>
                        </div>
                    </form>
                    <div class="row">
                       <div class="col-xs-4"></div>
                        <div class="col-xs-4">
                            <a class="btn btn-block bg-blue waves-effect" onclick="login()">LOGIN</a>
                        </div>
                    </div>
                    <div class="row m-t-15 m-b--20">
                       
                    </div>
               
            </div>
        </div>
    </div>

<script>
    function login(){
        document.getElementById('petowner_login').submit();
    }
</script>




    <!-- Jquery Core Js -->
    <script src="../../plugins/jquery/jquery.min.js"></script>

